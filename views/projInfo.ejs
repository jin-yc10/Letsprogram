<form class="form-horizontal" method="post">
    <fieldset>

    <div class="page-header">
        <h1>
        <div class="row">
            <div class="span9">
                <%=proj.name%><small> · <%=proj.lang%> ·</small>
            </div>
            <div class="span2">
                <div class="btn-group">
                    <button class="btn btn-success"><b>I want in!</b></button>
                    <a href="/search" class="btn">More Projects...</a>
                </div>
            <div>
        </div>
        </h1>

    </div>
    <div class="well">
        <p><h4>last update at : <%=proj.lastUpdate.toString().substring(0,24) %>
            </h4></p>
        <p><h4>lead by: <a href="/u/<%=proj.admin%>"><%=proj.admin%>
                        </a></h4></p>
        <p><h4>members:
                <%proj.member.forEach(function(m){%>
                    <a href="/u/<%=m%>"><%=m%></a>,
                <%})%>
            </h4></p>
    </div>
    </fieldset>
</form>